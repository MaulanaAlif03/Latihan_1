package interhance;

/**
 *
 * @author Muhammad Maulana Alif
 */
public class Matematikacanggih {
    void modulus(int a, int b){
        System.out.println("modulus : "+a+"%"+b+"="+ (a%b) );
    }
    void pertambahan(int a, int b){
        System.out.println("pertambahan : "+a+"%"+b+"="+ (a%b));
    }
    void perkalian (int a, int b){
        System.out.println("perkalian : "+a+"%"+b+"="+ (a%b));
    }
}

package interhance;

/**
 *
 * @author Muhammad Maulana Alif
 */
public class Matematikacanggih {
    public static void main(String[] args) {
        Matematikacanggih galaumatematika = new Matematikacanggih();
        
        galaumatematika.pertambahan(15,15);
        galaumatematika.perkalian(5,8);
        galaumatematika.modulus(10,200);
    }
}
